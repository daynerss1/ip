<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Priority?>

<fx:root alignment="TOP_RIGHT"
         maxHeight="1.7976931348623157E308"
         maxWidth="1.7976931348623157E308"
         spacing="8.0"
         type="javafx.scene.layout.HBox"
         xmlns="http://javafx.com/javafx/17"
         xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Label fx:id="dialog" text="Label" wrapText="true" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS" />
        <ImageView fx:id="displayPicture" fitHeight="34.0" fitWidth="34.0" pickOnBounds="true" preserveRatio="true" />
    </children>
    <padding>
        <Insets bottom="4.0" left="8.0" right="8.0" top="4.0" />
    </padding>
</fx:root>
